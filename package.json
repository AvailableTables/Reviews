{
  "name": "table-open-reviews-component",
  "version": "",
  "description": "Reviews component for TableOpen project, which is replicating OpenTable's restaurant web page.",
  "main": "index.js",
  "author": "LO",
  "license": "ISC",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "react-dev": "webpack -d --env.SC_ATTR=reviews --watch",
    "server-dev": "nodemon server/index.js",
    "pgschema": "psql -U  lotter -f ./db/pg/pgschema.sql",
    "pgdummydata": "node ./db/pg/buildUserData.js && node ./db/pg/buildRestaurantData.js && node ./db/pg/buildReviewsData.js",
    "pgseed": "node ./db/pg/populatepg.js",
    "pgtables": "node ./db/pg/alterTables.js",
    "test": "jest ./spec/test.js",
    "fireArtillery": "artillery run spec/newrelic/load-test.yml",
    "redis-dummy": "node ./db/redis/buildRestaurantData.js && node ./db/redis/revComp.js",
    "redis-populate-rest": "cat redisRest.txt | redis-cli --pipe",
    "redis-populate-rev": "cat redisRevComp.txt | redis-cli --pipe",
    "seed-aws": "mysql -h tableopen-reviews-db-free.ctqqauvpyeqz.us-east-1.rds.amazonaws.com -P 3306 -u mjones61692 -p < ./db/pg/reviewsSchema.sql && node ./db/pg/buildUserData.js && node ./db/pg/buildRestaurantData.js && node ./db/pg/buildReviewsData.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AvailableTables/Reviews"
  },
  "homepage": "https://github.com/AvailableTables/Reviews#README",
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.23.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "webpack": "^2.2.1",
    "webpack-cli": "^3.1.0",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.2",
    "body-parser": "^1.17.2",
    "compression": "^1.7.3",
    "csv-write-stream": "^2.0.0",
    "express": "^4.15.0",
    "express-handlebars": "^3.0.0",
    "gzip": "^0.1.0",
    "ioredis": "^4.2.0",
    "jest": "^23.6.0",
    "lzutf8": "^0.5.5",
    "morgan": "^1.9.1",
    "mysql": "^2.15.0",
    "newrelic": "^4.11.0",
    "nodemon": "^1.18.6",
    "pg": "^7.6.1",
    "pg-copy-streams": "^1.2.0",
    "puppeteer": "^1.9.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "redis": "^2.8.0",
    "redis-streams": "^1.1.0",
    "sequelize": "^4.41.1",
    "styled-components": "^4.0.2",
    "zlib": "^1.0.5"
  }
